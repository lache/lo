{
    "extends": [
      "standard",
      "prettier",
      "prettier/standard"
    ],
    "plugins": [
      "prettier",
      "standard",
      "import",
      "node",
      "promise"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    }
  }